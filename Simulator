 local isRunning = false
 --------------------------------------------------
 --                 Toggles                      --
 --------------------------------------------------
 local Toggle = MainTab:CreateToggle({
    Name = "Auto Click",
    CurrentValue = false,
    Flag = "Toggle1",
    Callback = function(Value)
        isRunning = Value

        if isRunning then
            while isRunning do
                game:GetService("ReplicatedStorage").Remote.Event.Game:FindFirstChild("[C-S]PlayerClick"):FireServer()
                wait()
            end
        end
    end,
})


 local Toggle = MainTab:CreateToggle({
     Name = "Auto Best Sword",
     CurrentValue = false,
     Flag = "Toggle2",
     Callback = function(Value)
         isRunning = Value 
 
         if isRunning then
             while isRunning do
                local args = {[1] = "p1"}
                game:GetService("ReplicatedStorage").Remote.Event.Skin:FindFirstChild("[C-S]TryEquipBestSkin"):FireServer(unpack(args))
                 wait(5)
             end
         end
     end,
 })

 local Toggle = MainTab:CreateToggle({
    Name = "Auto Best Ki",
    CurrentValue = false,
    Flag = "Toggle3",
    Callback = function(Value)
        isRunning = Value 

        if isRunning then
            while isRunning do
