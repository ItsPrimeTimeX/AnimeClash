})

local WinTab = Window:CreateTab("Auto WinsüèÜ", nil)
local WinSection = WinTab:CreateSection("Beat the NPC once first, then turn on auto")

local Toggle = WinTab:CreateToggle({
   Name = "Auto Win",
   CurrentValue = false,
   Flag = "Toggle6",
   Callback = function(Value)
       isRunning = Value 

       if isRunning then
           while isRunning do
               local args = {[1] = true}
               game:GetService("ReplicatedStorage").Remote.Event.Game:FindFirstChild("[C-S]PlayerPKEnd"):FireServer(unpack(args))
               wait(5)
           end
       end
   end,
   })

